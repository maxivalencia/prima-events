{% extends 'base.html.twig' %}

{% block title %}Taux de Refacturation{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Taux de Refacturation</h1>
    </div>
    <hr>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ taux_refacturation.id }}</td>
            </tr>
            <tr>
                <th>Taux</th>
                <td>{{ taux_refacturation.taux }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('taux_refacturation_index') }}" class="btn btn-primary">Retour</a>
    <br><br>
    <a href="{{ path('taux_refacturation_edit', {'id': taux_refacturation.id}) }}" class="btn btn-warning">Modifier</a>
    <br><br>
    {{ include('taux_refacturation/_delete_form.html.twig') }}
</div>
{% endblock %}
