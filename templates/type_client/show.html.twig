{% extends 'base.html.twig' %}

{% block title %}Type de Client{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Type de Client</h1>
    </div>
    <hr>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ type_client.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ type_client.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('type_client_index') }}" class="btn btn-primary">Retour</a>
    <br><br>
    <a href="{{ path('type_client_edit', {'id': type_client.id}) }}" class="btn btn-warning">Modifier</a>
    <br><br>
    {{ include('type_client/_delete_form.html.twig') }}
</div>
{% endblock %}
