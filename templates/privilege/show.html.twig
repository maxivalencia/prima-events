{% extends 'base.html.twig' %}

{% block title %}Privilege{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Privilege</h1>
    </div>
    <hr>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ privilege.id }}</td>
            </tr>
            <tr>
                <th>Privilege</th>
                <td>{{ privilege.privilege }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('privilege_index') }}" class="btn btn-primary">Retour</a>
    <br><br>
    <a href="{{ path('privilege_edit', {'id': privilege.id}) }}" class="btn btn-warning">Modifier</a>
    <br><br>
    {{ include('privilege/_delete_form.html.twig') }}
</div>
{% endblock %}
