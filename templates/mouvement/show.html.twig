{% extends 'base.html.twig' %}

{% block title %}Mouvement{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Mouvement</h1>
    </div>
    <hr>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mouvement.id }}</td>
            </tr>
            <tr>
                <th>Mouvement</th>
                <td>{{ mouvement.mouvement }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('mouvement_index') }}" class="btn btn-primary">Retour</a>
    <br><br>
    <a href="{{ path('mouvement_edit', {'id': mouvement.id}) }}" class="btn btn-warning">Modifier</a>
    <br><br>
    {{ include('mouvement/_delete_form.html.twig') }}
</div>
{% endblock %}
